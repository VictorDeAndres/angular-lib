{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "circular-graphs": {
      "projectType": "library",
      "root": "projects/circular-graphs",
      "sourceRoot": "projects/circular-graphs/src",
      "prefix": "ngl",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/circular-graphs/tsconfig.lib.json",
            "project": "projects/circular-graphs/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run",
          "options": {
            "no-cache": true,
            "reporters": "jest-junit"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/circular-graphs/tsconfig.lib.json",
              "projects/circular-graphs/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }},
  "defaultProject": "circular-graphs"
}